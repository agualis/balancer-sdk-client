{
  "name": "balancer-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "local-sdk": "rimraf ./node_modules/@balancer-labs && npm i -D ../balancer-sdk/balancer-js",
    "build-local": "cd ../balancer-sdk/balancer-js && yarn build && cd ../../balancer-sdk-client && npm run local-sdk && npm run build"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@balancer-labs/sdk": "file:../balancer-sdk/balancer-js",
    "@balancer-labs/sor": "file:../balancer-sdk",
    "@vitejs/plugin-vue": "^4.0.0",
    "bignumber.js": "^9.1.1",
    "rimraf": "^4.3.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  }
}
